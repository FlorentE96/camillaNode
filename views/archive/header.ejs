


<script src="main.js"></script>


<div id="header" class="section">
    <div id="title" style="display: flex; align-items: center; justify-content: left;">
        <img src="icon.jpg" alt="" style="height: 50px; margin:10px">
        <span>CamillaNode</span>
    </div> 
</div>    

<script>     
                
        // Apply styles from preferences 
        document.documentElement.style.setProperty('--header-hue',headerHue);
        document.documentElement.style.setProperty('--main-hue',mainHue);
        document.documentElement.style.setProperty('--eqparamFontSize',eqparamFontSize);
        document.documentElement.style.setProperty('--saturation',saturation+'%');
        document.documentElement.style.setProperty('--levelmeter_height',levelmeterHeight+'px');
        document.documentElement.style.setProperty('--configCount',configCount);
        
        // console.log("Header script")
        const menuItems = document.getElementsByClassName('menuItem')


        // console.log(menuItems)
        for (i=0;i<menuItems.length;i++) {            
            menuItems[i].addEventListener('click',function(){
                window.location.href=this.getAttribute('target');
            })
        }
    
</script>


